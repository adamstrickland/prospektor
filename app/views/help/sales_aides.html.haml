#sales_aides.help.dialog.modal
  %h2 Sales Aides
  %ul.help
    -@entries.each_pair do |hdg, grp|
      %li
        =hdg
        %ul.help
          -grp.each_pair do |name, url|
            %li=link_to name, url, :target => '_blank'
    %li=link_to 'Glossary of Terms', "#{@www_sa}/Glossary.htm", :target => '_blank'